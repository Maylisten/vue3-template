{
  "name": "vue3-template",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">= 18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "cli:build": "pnpm run -C cli build",
    "cli:dev": "pnpm run -C cli dev",
    "release": "pnpm run cli:build && pnpm -C cli release",
    "lint": "eslint .",
    "fix": "eslint . --fix"
  },
  "dependencies": {
    "pinia": "^2.2.2",
    "vue": "^3.4.37",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@eslint/js": "^10.0.0",
    "@types/node": "^22.5.4",
    "@vitejs/plugin-vue": "^5.1.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.9.0",
    "less": "^4.2.0",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.4.0",
    "vite": "^5.4.1",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^2.0.29"
  }
}
